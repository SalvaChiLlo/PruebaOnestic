{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "airbnb-typescript/base"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "rules": {
        "prefer-destructuring": "warn",
        "no-unused-vars": "warn",
        "max-len": "off",
        "no-console": "off",
        "@typescript-eslint/no-use-before-define": "off"
    }
}